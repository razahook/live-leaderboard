{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  },
  "ignoreCommand": "echo 'Skipping build step'",
  "buildCommand": "echo 'No build step needed'",
  "outputDirectory": "."
}